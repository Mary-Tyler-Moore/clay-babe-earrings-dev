{
  "name": "gatsby-ecommerce-netlify-cms-tailwindcss",
  "private": true,
  "description": "A storefront with Gatsby, Stripe & Netlify CMS.",
  "version": "0.0.1",
  "author": "Fullstack Rocket",
  "scripts": {
    "develop": "gatsby develop",
    "start": "NODE_ENV=development run-p start:**",
    "start:app": "yarn run develop",
    "start:lambda": "netlify-lambda serve -c ./netlify.webpack.functions.js src/lambda",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build -c ./netlify.webpack.functions.js src/lambda",
    "build-production": "NODE_ENV=production yarn build",
    "build-staging": "NODE_ENV=staging yarn build",
    "clean": "gatsby clean",
    "deploy": "yarn build-production && netlify deploy --prod",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\""
  },
  "dependencies": {
    "@rehooks/local-storage": "^2.3.0",
    "@stripe/stripe-js": "1.5.0",
    "dotenv-webpack": "^1.7.0",
    "gatsby": "2.21.1",
    "gatsby-image": "2.4.0",
    "gatsby-plugin-manifest": "2.4.1",
    "gatsby-plugin-netlify": "2.3.0",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-netlify-cms": "4.3.0",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-postcss": "2.3.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "3.3.0",
    "gatsby-plugin-sass": "2.3.0",
    "gatsby-plugin-sharp": "2.6.0",
    "gatsby-plugin-sitemap": "2.4.0",
    "gatsby-remark-copy-linked-files": "2.3.1",
    "gatsby-remark-images": "3.3.0",
    "gatsby-remark-relative-images": "0.3.0",
    "gatsby-source-filesystem": "2.3.0",
    "gatsby-source-stripe": "^3.0.3",
    "gatsby-transformer-remark": "2.8.0",
    "gatsby-transformer-sharp": "2.5.0",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms-app": "^2.9.6",
    "netlify-cms-media-library-cloudinary": "^1.3.2",
    "netlify-cms-media-library-uploadcare": "^0.5.2",
    "node-sass": "4.14.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "6.0.0",
    "react-icons": "^3.9.0",
    "react-share": "^4.1.0",
    "slugify": "^1.4.0",
    "stripe": "8.47.1",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-gatsby-standard": "^2.2.0",
    "eslint-plugin-react": "^7.14.3",
    "http-proxy-middleware": "1.0.3",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "tailwindcss": "1.3.5"
  }
}
